{
  "name": "bug-bash-helper",
  "version": "1.0.0",
  "private": true,
  "description": "Helper bot to handle Bug Bash challenge.",
  "repository": "https://github.com/maxceem/bug-bash-helper.git",
  "homepage": "https://github.com/maxceem/bug-bash-helper",
  "bugs": "https://github.com/maxceem/bug-bash-helper/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "outdent": "^0.7.0",
    "probot": "^12.2.8"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^11.4.0",
    "nodemon": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
